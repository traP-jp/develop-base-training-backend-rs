{
	"image": "mcr.microsoft.com/devcontainers/rust:1-1-bullseye",
	"features": {
		"ghcr.io/devcontainers/features/rust:1": {}
	},
    "customizations": {
        "codespaces": {
            "openFiles": [
                "src/main.rs"
            ]
        },
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer"
            ]
        }
    },
    "forwardPorts": [8080],
    "portsAttributes": {
        "8080": {
            "visibility": "public"
        }
    }
}
